[node name="RichTextLabel5" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : Lady Yuyuko… This is all my fault. I couldn't retrieve the Soul Torch in time."

[node name="RichTextLabel6" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : The spirits may be huddled inside the Hakurei Shrine for now, but it's only a matter of time before things get out of hand."

[node name="RichTextLabel7" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : Shall I guide them back to the Netherworld?"

[node name="RichTextLabel8" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : No need. Look."

[node name="RichTextLabel9" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : Incredible… They're passing on on their own…"

[node name="RichTextLabel10" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : Unlike us, these spirits exist in the Netherworld in the form of raw emotions."

[node name="RichTextLabel11" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : These emotions are the last vestige of their existence. Should they be consumed, or in this case felt…"

[node name="RichTextLabel12" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : Nothing would be left to hold them back."

[node name="RichTextLabel13" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : And now they rest in peace."

[node name="RichTextLabel14" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : See? The situation resolved itself. So don't beat yourself up."

[node name="RichTextLabel15" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : …"

[node name="RichTextLabel16" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : You knew this from the start. This was your plan all along, wasn't it?"

[node name="RichTextLabel17" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : Fufu~ Don't give me that look, Youmu dear."

[node name="RichTextLabel18" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : It's true that the Netherworld was getting a little crowded lately. But who knows?"

[node name="RichTextLabel19" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Yuyuko : It could have just been a fortunate coincidence."

[node name="RichTextLabel20" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Youmu : And somehow, I don't believe you."

[node name="RichTextLabel21" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "As expected, the cycle of Samsara was preserved."

[node name="RichTextLabel22" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "The energy that drives a spirit's attachment to existence itself is very nuanced."

[node name="RichTextLabel23" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
text = "Perhaps for some, all it takes to reach Nirvana..."

[node name="RichTextLabel24" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
