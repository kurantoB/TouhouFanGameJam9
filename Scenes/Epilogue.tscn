[node name="RichTextLabel4" type="RichTextLabel" parent="CanvasLayer/HUD/MessageBox/Control/Epilogue"]
visible = false
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/normal_font = ExtResource( 5 )
custom_colors/default_color = Color( 0, 0, 0, 1 )
